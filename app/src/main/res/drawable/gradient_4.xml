<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:gradientRadius="450"
        android:centerX="50%"
        android:centerY="50%"
        android:centerColor="#D60000"
        android:startColor="#7C0000"
        android:endColor="#6E0000"
        android:type="radial"
        />
    <corners
        android:radius="14dp"
        />
    <stroke
        android:width="2dp"
        android:color="#8F8F8F"
        />
    <size
        android:width="200dp"
        android:height="60dp"
        />
</shape>